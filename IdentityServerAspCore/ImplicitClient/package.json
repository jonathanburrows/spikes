{
  "name": "implicit-client",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "init.dev": "npm link ../AngularCommon",
    "build.dev": "ng build --progress=false --output-path=./dist/dev",
    "serve.dev": "ng serve --progress=false --output-path=./dist/serve --open --port 5003",
    "build.prod": "ng build --prod --aot --progress=false --output-path=./dist/prod",
    "serve.prod": "ng serve --prod --aot --progress=false --output-path=./dist/serve --open --port 5003",
    "test": "ng test --watch=false",
    "init.e2e": "webdriver-manager update --standalone false --gecko false",
    "run.e2e": "protractor",
    "e2e": "npm run init.e2e && npm-run-all -p serve.dev run.e2e",
    "lint": "ng lint",
    "ng": "ng"
  },
